{
    "id": "org.kde.skanpage",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.15-23.08",
    "sdk": "org.kde.Sdk",
    "rename-icon": "skanpage",
    "command": "skanpage",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=all",
        "--share=network",
        "--system-talk-name=org.freedesktop.Avahi"
    ],
    "cleanup": [
        "*.a",
        "*.la",
        "/include",
        "/lib/cmake",
        "/lib/pkgconfig",
        "/mkspecs",
        "/share/doc",
        "/share/man",
        "/share/qlogging-categories5"
    ],
    "modules": [
        "shared-modules/libusb/libusb.json",
        {
            "name": "avahi",
            "buildsystem": "autotools",
            "config-opts": [
                "--disable-gtk",
                "--disable-qt3",
                "--disable-qt4",
                "--disable-mono",
                "--disable-monodoc",
                "--with-distro=none",
                "--disable-pygobject",
                "--disable-python-dbus",
                "--disable-python",
                "--disable-manpages",
                "--disable-libdaemon",
                "--disable-daemon",
                "--enable-compat-libdns_sd",
                "--disable-libevent"
            ],
            "sources": [
                {
                    "type": "git",
                    "url": "https://github.com/lathiat/avahi",
                    "tag": "v0.8",
                    "x-checker-data": {
                        "type": "git",
                        "tag-pattern": "^v([\\d.]+)$"
                    },
                    "commit": "f060abee2807c943821d88839c013ce15db17b58"
                }
            ]
        },
        {
            "name": "sane",
            "buildsystem": "autotools",
            "config-opts": [
                "--with-usb",
                "--with-v4l",
                "--with-gphoto2",
                "--enable-pthread",
                "--enable-pnm-backend",
                "--enable-pthread",
                "--enable-avahi"
            ],
            "sources": [
                {
                    "type": "git",
                    "url": "https://gitlab.com/sane-project/backends",
                    "tag": "1.3.0",
                    "x-checker-data": {
                        "type": "git",
                        "tag-pattern": "^([\\d.]+)$"
                    },
                    "commit": "7088afe04dc83395af599b6c224b020a142e64d8"
                }
            ]
        },
        {
            "name": "sane-airscan",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "git",
                    "url": "https://github.com/alexpevzner/sane-airscan",
                    "tag": "0.99.27",
                    "commit": "053f5654cd4d16f077fe5cb40d32004aaa321462"
                }
            ]
        },
        {
            "name": "ksanecore",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.kde.org/stable/release-service/23.08.5/src/ksanecore-23.08.5.tar.xz",
                    "sha256": "3d19c4caf13799b4bc835d18d77668ebb86771ba5128de3497a43302371d9ba2",
                    "x-checker-data": {
                        "type": "anitya",
